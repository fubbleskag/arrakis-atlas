# Copy this file to .env.local and fill in your Firebase project configuration.
# You can find these values in your Firebase project settings.
# After creating or updating this file, you must restart your development server.

NEXT_PUBLIC_FIREBASE_API_KEY="YOUR_API_KEY"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="YOUR_AUTH_DOMAIN"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="YOUR_PROJECT_ID"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="YOUR_STORAGE_BUCKET"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="YOUR_MESSAGING_SENDER_ID"
NEXT_PUBLIC_FIREBASE_APP_ID="YOUR_APP_ID"

# This is optional, but recommended for consistent shareable links from your local dev environment.
# If not set, it will default to window.location.origin.
# For production, you should set this in your hosting provider's environment variables.
# Example: NEXT_PUBLIC_APP_URL="http://localhost:9002"
NEXT_PUBLIC_APP_URL=""
